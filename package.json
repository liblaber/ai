{
  "name": "liblab.ai",
  "description": "An AI-powered developer tool platform that accelerates development workflows and lowers barriers to building with AI",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "quickstart": "pnpm run setup && pnpm run docker:start",
    "setup": "pnpm i && pnpm run prisma:generate && tsx setup.ts",
    "build": "next build",
    "dev": "pnpm prisma generate && tsx scripts/dev-with-db.ts && tsx pre-start.ts && next dev",
    "test": "jest",
    "start": "tsx setup-tunnel.ts && tsx run-migrations.ts && next start",
    "prod": "pnpm run build && pnpm run start",
    "dockerdev": "tsx pre-start.ts && next dev",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app",
    "lint:fix": "pnpm exec prettier --check . --write",
    "docker:start": "docker compose up -d && tsx scripts/docker-start-prompt.ts",
    "docker:rebuild": "docker compose up -d --build && tsx scripts/docker-start-prompt.ts",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d && tsx scripts/docker-start-prompt.ts",
    "docker:stop": "docker compose down",
    "db:start": "docker compose -f docker-compose.db.yml up -d",
    "db:stop": "docker compose -f docker-compose.db.yml down",
    "db:restart": "docker compose -f docker-compose.db.yml restart",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "lint-staged": "lint-staged",
    "preview": "pnpm run build && pnpm run start",
    "prepare": "husky",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "test:e2e": "tsx scripts/run-e2e-tests.ts",
    "test:e2e:headed": "tsx scripts/run-e2e-tests.ts headed",
    "test:e2e:ui": "tsx scripts/run-e2e-tests.ts ui",
    "test:e2e:debug": "tsx scripts/run-e2e-tests.ts debug",
    "test:e2e:headless": "tsx scripts/run-e2e-tests.ts headless"
  },
